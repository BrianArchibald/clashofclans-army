{
  "name": "clashofclans-army",
  "version": "0.0.0-development",
  "description": "Get random Clash of Clans troop",
  "main": "src/index.js",
  "scripts": {
    "test": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "test:watch": "mocha src/index.test.js -w",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "git-cz",
    "check-coverage":
      "istanbul check-coverage --statements 100 --branches 100 --function 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chgasparoto/clashofclans-army.git"
  },
  "keywords": ["random", "clash of clans"],
  "author":
    "Cleber Gasparoto <chgasparoto@gmail.com> (https://github.com/chgasparoto)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chgasparoto/clashofclans-army/issues"
  },
  "homepage": "https://github.com/chgasparoto/clashofclans-army#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.2",
    "istanbul": "0.4.5",
    "mocha": "5.0.0",
    "semantic-release": "^12.4.1",
    "travis-deploy-once": "^4.3.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
